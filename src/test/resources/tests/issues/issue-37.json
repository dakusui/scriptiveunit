{
  "$extends": [],
  "description": "Lifecycle",
  "factorSpace": {
    "factors": {
      "num": [2, 10],
      "base":["a", "b"]
    }
  },
  "setUpBeforeAll": ["print","setUpBeforeAll"],
  "setUp": ["print", ["format", "setUp:base=%s", ["attr", "base"]]],
  "testOracles": [
    {
      "description": "Oracle A",
      "given": ["not", ["always"]],
      "when": ["print",  ["format", "Oracle A:%s", ["attr", "num"]]],
      "then": ["always"]
    },
    {
      "description": "Oracle B",
      "given": ["always"],
      "when": ["print",  ["format", "Oracle B:%s", ["attr", "num"]]],
      "then": ["always"]
    },
    {
      "description": "Non-terminating floating number created by division can be handled",
      "given": ["always"],
      "when": ["print",  ["format", "Oracle A:%s", ["attr", "num"]]],
      "then": ["and",
        [">", 0.34, ["/", 1, 4]],
        ["<", 0.33, ["/", 1, 3]]
      ]
    }
  ],
  "tearDown": ["print", ["format", "tearDown:base=%s", ["attr", "base"]]],
  "tearDownAfterAll": ["print", "tearDownAfterAll"]
}
